{
  "name": "erm-react",
  "version": "1.0.0",
  "description": "A noddy React app",
  "main": "app.js",
  "dependencies": {
    "history": "^2.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build:dev": "browserify -v -t [ babelify ] src/app.js -o src/bundle.js",
    "build:dev1": "browserify -r react -r react-dom -v -t [ babelify ]  -o src/reactbundle.js",
    "build:dev2": "browserify -x react -x react-dom -v -t [ babelify ] src/app.js -o src/appbundle.js",
    "build:dev3": "browserify -x react -x react-dom -v -t [ babelify ] src/overview.js -o src/overviewbundle.js",
    "build:watch": "watchify -v -t [ babelify ] src/app.js -o src/bundle.js",
    "build:release": "browserify -v -t [ babelify ] src/app.js | uglifyjs > src/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Joe Lloyd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/advanced-joelloyd/erm-react"
  }
}
